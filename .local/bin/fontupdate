curl -o "$HOME/.local/share/fonts/JetBrainsMono" -LJO https://raw.githubusercontent.com/ryanoasis/nerd-fonts/master/patched-fonts/JetBrainsMono/Ligatures/Bold/JetBrainsMonoNerdFont-Medium.ttf
curl -o "$HOME/.local/share/fonts/JetBrainsMono" -LJO https://raw.githubusercontent.com/ryanoasis/nerd-fonts/master/patched-fonts/JetBrainsMono/Ligatures/Bold/JetBrainsMonoNerdFont-Regular.ttf
curl -o "$HOME/.local/share/fonts/JetBrainsMono" -LJO https://raw.githubusercontent.com/ryanoasis/nerd-fonts/master/patched-fonts/JetBrainsMono/Ligatures/Bold/JetBrainsMonoNerdFont-SemiBold.ttf
